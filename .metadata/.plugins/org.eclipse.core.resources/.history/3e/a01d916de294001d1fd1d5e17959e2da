import java.util.Locale;
import java.util.Scanner;

public class Main {

	public static void main(String[] args) {

		Locale.setDefault(Locale.US);
		Scanner sc = new Scanner(System.in);

		char tipo;
		int casos, numCobaias, coelhos, ratos, sapos, totalCobaias;
		double pCoelhos, pRatos, pSapos; 

		System.out.print("Quantos casos de teste serao digitados? ");
		casos = sc.nextInt();

		coelhos = 0;
		ratos = 0;
		sapos = 0;

		for (int i = 0; i < casos; i++) {

			System.out.print("Quantidade de cobaias: ");
			numCobaias = sc.nextInt();

			System.out.print("Tipo de cobaia: ");
			tipo = sc.next().charAt(0);

			if (tipo == 'C') {
				coelhos = coelhos + numCobaias;
			} 
			else if (tipo == 'R') {
				ratos = ratos + numCobaias;
			} 
			else {
				sapos = sapos + numCobaias;
			}
		}

		totalCobaias = coelhos + ratos + sapos;
		
		pCoelhos = (double) coelhos / totalCobaias * 100.0;
		pRatos = (double) ratos / totalCobaias * 100.0;
		pSapos = (double) sapos / totalCobaias * 100.0;

		System.out.println();
		System.out.println("RELATORIO FINAL:");
		System.out.printf("Total: %d cobaias\n", totalCobaias);
		System.out.printf("Total de coelhos: %d\n", coelhos);
		System.out.printf("Total de ratos: %d\n", ratos);
		System.out.printf("Total de sapos: %d\n", sapos);
		System.out.printf("Percentual de coelhos: %.2f", pCoelhos);
		System.out.printf("Percentual de ratos: %.2f", pRatos);
		System.out.printf("Percentual de sapos: %.2f", pSapos);
		

		sc.close();

	}

}
